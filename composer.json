{
  "name": "elementareteilchen/groupdelegation",
  "type": "typo3-cms-extension",
  "description": "Allows generating Sub-Admin be_user by groups which can handle access of other be_user by groups",
  "version": "5.0.1",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Sebastian Mueller",
      "role": "Developer"
    },
    {
      "name": "Marcus Schwemer",
      "role": "Developer"
    },
    {
      "name": "Andreas Sommer",
      "role": "Developer"
    },
      {
          "name": "Franz Kugelmann",
          "role": "Developer"
      }
  ],
  "require": {
    "typo3/cms-core": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "ElementareTeilchen\\Groupdelegation\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/groupdelegation": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "groupdelegation"
    }
  }
}
